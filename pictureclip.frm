Private Sub Form_Load()
    Mix
    For x = 0 To 49
        Picture1(x + 1) = Clip1.GraphicCell(Pic(x))
        If Pic(x) = 49 Then Bla = (x + 1)
    Next x

End Sub

Private Sub mnuShow_Click()
Form2.Show
End Sub

Private Sub Picture1_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
    If (Bla - 10) > 0 And KeyCode = vbKeyDown Then
       byt = Bla - 10
       Call Swap(byt, Bla)
       Bla = byt
    End If

    If Bla <= 40 And KeyCode = vbKeyUp Then
       byt = Bla + 10
       Call Swap(byt, Bla)
       Bla = byt
    End If

    If Bla < 50 And KeyCode = vbKeyLeft Then
       byt = Bla + 1
       Call Swap(byt, Bla)
       Bla = byt
    End If

    If Bla >= 2 And KeyCode = vbKeyRight Then
       byt = Bla - 1
       Call Swap(byt, Bla)
       Bla = byt
    End If
    
End Sub
