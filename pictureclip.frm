Public Sub Swap(i As Integer, Blue As Integer)
    Picture1(0).Picture = Picture1(i).Picture
    Picture1(i).Picture = Picture1(Blue).Picture
    Picture1(Blue).Picture = Picture1(0).Picture
End Sub
Sub Mix()
Dim Rand As Integer
Dim Have_It As Boolean
Dim i As Integer
Dim counter As Integer

counter = 0
Randomize
    Do
        Do
            Rand = Int(Rnd * 49) + 1
            Have_It = False
                For i = 0 To counter
                    If Pic(i) = Rand Then
                        Have_It = True
                    End If
                Next i
                If Have_It = True Then Rand = Int(Rnd * 49) + 1
            Loop Until Have_It = False
        Pic(counter) = Rand
        
        counter = counter + 1
        Label1 = counter
        DoEvents
        
    Loop Until counter = 49

    
End Sub

